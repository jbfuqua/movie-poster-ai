{
“version”: 2,
“name”: “movie-poster-ai”,
“builds”: [
{
“src”: “server.js”,
“use”: “@vercel/node”
},
{
“src”: “public/**/*”,
“use”: “@vercel/static”
}
],
“routes”: [
{
“src”: “/api/(.*)”,
“dest”: “/server.js”
},
{
“src”: “/(.*)”,
“dest”: “/public/$1”
}
],
“env”: {
“NODE_ENV”: “production”
},
“functions”: {
“server.js”: {
“maxDuration”: 60
}
}
}